{
  "expo": {
    "name": "mark.AI",
    "slug": "mark-ai-marketplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "markai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markai.marketplace"
    },
    "android": {
      "package": "com.markai.marketplace",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
